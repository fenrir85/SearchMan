<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - NWL Employee Console | National Western Life Insurance</title>
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")
    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
</head>
<body>
    <nav class="navbar navbar-expand-xl navbar-default main-nav" data-toggle="affix">
        <div class="container-fluid">
            <h4>
                <a class="navbar-brand" href="//nationalwesternlife.com"><img src="/images/nwl-logo.png" alt="National Western Life" width="120"></a>
            </h4>
            <div class="collapse navbar-collapse align" id="navbarSupportedContent">
                <h3 class="mr-auto"><span class="oi oi-aperture oi-2x"></span> <a href="/">Employee Console</a></h3>
                <div class="d-flex" id="liProfileInfo">

                    <h5 class="navbar-text navbar-right" id="userDisplayName"><span class="oi oi-person"></span> Mr. Smile (MVP)</h5>
                </div>
            </div>
        </div>
    </nav>
    >@Html.ActionLink("Search", "Search", "Home")
    <div class="container body-content">
        @RenderBody()
    </div>
    <hr />
    <footer>
        <p>National Western Life Insurance </p>
    </footer>


</body>
</html>
