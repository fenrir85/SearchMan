@{
    ViewBag.Title = "Home Page";
}

<!DOCTYPE html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - My ASP.NET Application</title>
    

</head>
<html>
<head>
    <meta name="viewport" content="width=device-width" />
</head>
<body>

    <form name="search" action="/Home/Searching" method="post">
        <div class="search d-flex">
            <input id="searchBox" type="text" name="searchKeyword" class="form-control " required placeholder="Search by name, agent code or policy number">
            <input class="btn btn-primary" type="submit" value="Find" id="btnSearch">
        </div>

    </form>

    <table class="display" id="coneo">
        <thead>
            <tr>
                <th>Column 1</th>
                <th>Column 2</th>
            </tr>
        </thead>
        <tbody>
            <tr>
                <td>Row 1 Data 1</td>
                <td>Row 1 Data 2</td>
            </tr>
            <tr>
                <td>Row 2 Data 1</td>
                <td>Row 2 Data 2</td>
            </tr>
        </tbody>
    </table>

    @Scripts.Render("~/page/search")


</body>
</html>
